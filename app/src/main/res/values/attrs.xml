<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleSeekBar">
        <attr name="wheel_max_process" format="integer" />
        <attr name="wheel_cur_process" format="integer" />
        <attr name="wheel_reached_color" format="color" />
        <attr name="wheel_reached_width" format="dimension" />
        <attr name="wheel_reached_has_corner_round" format="boolean"/>
        <attr name="wheel_unreached_color" format="color" />
        <attr name="wheel_unreached_width" format="dimension" />
        <attr name="wheel_pointer_color" format="color" />
        <attr name="wheel_pointer_radius" format="dimension" />
        <attr name="wheel_has_pointer_shadow" format="boolean" />
        <attr name="wheel_has_wheel_shadow" format="boolean" />
        <attr name="wheel_pointer_shadow_radius" format="dimension" />
        <attr name="wheel_shadow_radius" format="dimension" />
        <attr name="wheel_has_cache" format="boolean" />
        <attr name="wheel_can_touch" format="boolean" />
        <attr name="wheel_scroll_only_one_circle" format="boolean" />
        <attr name="wheel_start_angle" format="float"/>
        <attr name="wheel_current_angle" format="float"/>
        <attr name="pointcolor" format="color"/>
        <attr name="numsize" format="integer"/>
        <attr name="numcolor" format="color"/>
        <attr name="module" format="string"/>
        <attr name="slide" format="boolean"/>
        <attr name="week" format="integer"/>
        <attr name="deviceId" format="string"/>
        <attr name="mCurrentAngle" format="integer"/>
        <attr name="first" format="boolean"/>
        <attr name="mCurAngle" format="integer"/>
        <attr name="end" format="integer"/>
        <attr name="workMode" format="string"/>
        <attr name="output" format="string"/>
        <attr name="semicircle_module" format="string"/>
    </declare-styleable>
    <declare-styleable name="DashboardView">
        <attr name="radius" format="dimension"/>
        <attr name="startAngle" format="integer"/>
        <attr name="sweepAngle" format="integer"/>
        <attr name="bigSliceCount" format="integer"/>
        <attr name="sliceCountInOneBigSlice" format="integer"/>
        <attr name="arcColor" format="color"/>
        <attr name="measureTextSize" format="dimension"/>

        <attr name="headerTitle" format="string"/>
        <attr name="headerTextSize" format="string"/>
        <attr name="headerRadius" format="dimension"/>
        <attr name="pointerRadius" format="dimension"/>
        <attr name="circleRadius" format="dimension"/>
        <attr name="realTimeValue" format="float"/>
        <attr name="stripeWidth" format="dimension"/>
        <attr name="stripeMode" >
            <enum name="normal" value="0"/>
            <enum name="inner" value="1"/>
            <enum name="outer" value="2"/>
        </attr>
        <attr name="bgColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="MySeekBar">
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textBackground" format="reference" />
        <attr name="textOrientation" format="enum">
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DiffuseView">
        <attr name="diffuse_color"/>
        <attr name="diffuse_coreColor"/>
        <attr name="diffuse_coreImage"/>
        <attr name="diffuse_coreRadius"/>
        <attr name="diffuse_width"/>
        <attr name="diffuse_maxWidth"/>
        <attr name="diffuse_speed"/>
    </declare-styleable>


    <declare-styleable name="RaidoTextView">
        <attr name="radioTitleText" format="string" />
        <attr name="radioBackground" format="color"/>
        <attr name="radioTitleTextSize" format="dimension"/>
        <attr name="radioTitleTextColor" format="color"/>
        <attr name="radioCornerSize" format="integer"/>
    </declare-styleable>
</resources>
